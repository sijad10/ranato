<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <lifecycle>
        <phase-listener>pe.gob.sucamec.sistemabase.seguridad.LoginPhaseListener</phase-listener>
    </lifecycle>
    <application>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>
        <factory>
            <exception-handler-factory>
                org.primefaces.application.exceptionhandler.PrimeExceptionHandlerFactory
            </exception-handler-factory>
        </factory>
        <resource-bundle>
            <base-name>/BundleBDIntegrado</base-name>
            <var>BundleBDIntegrado</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundlePrueba</base-name>
            <var>bundlePrueba</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleCydoc</base-name>
            <var>bundleCydoc</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleDisca</base-name>
            <var>bundleDisca</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleSistemaBase</base-name>
            <var>bundleSistemaBase</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleNotificacion</base-name>
            <var>bundleNotificacion</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleReportes</base-name>
            <var>bundleReportes</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleEpp</base-name>
            <var>bundleEpp</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleGeppLme</base-name>
            <var>BundleGeppLme</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleGeppLmpp</base-name>
            <var>BundleGeppLmpp</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleGamac</base-name>
            <var>bundleGamac</var>
        </resource-bundle>        
        <navigation-handler>org.primefaces.mobile.application.MobileNavigationHandler</navigation-handler>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleGeppGte</base-name>
            <var>bundleGeppGte</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundlet</base-name>
            <var>bundlet</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleGamacCVMuni</base-name>
            <var>bundleGamacCVMuni</var>
        </resource-bundle>        
        <resource-bundle>
            <base-name>/BundleRenagi</base-name>
            <var>bundleRenagi</var>
        </resource-bundle>        
        <resource-bundle>
            <base-name>/BundleCitas</base-name>
            <var>bundleCitas</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/BundleMicroservicios</base-name>
            <var>BundleMicroservicios</var>
        </resource-bundle>
    </application>
</faces-config>
